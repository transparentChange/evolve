{
  "manifest_version": 2,
  "name": "Bookmarks",
  "version": "0.0.1",
  "browser_action": {
    "default_title": "Bookmarks",
    "default_popup": "index.html"
  },
  "background": {
    "page": "/index.html?page=background"
  },
  "page_action": {
    "default_icon": "assets/cross-circle.svg",
    "default_title": "Mark tab as complete and close",
    "show_matches": ["*://*/*"]
  },
  "permissions": ["bookmarks", "storage", "tabs", "http://localhost:8080/*", "http://*/", "<all_urls>"]

}
